<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.main {
			padding: 10px;
			padding-bottom: 30px;
		}
		
		.floatRight{
			float:right;
		}
	</ui:style>
	<g:DialogBox modal="true" animationEnabled="true" glassEnabled="true" autoHideEnabled="true">
		<g:HTMLPanel styleName="{style.main}" >
			<p ui:field="msg" style="margin-left: 10px;"></p>
			<div>
				<g:Button ui:field='okButton' styleName="{style.floatRight}">Fermer</g:Button>
			</div>
		</g:HTMLPanel>
	</g:DialogBox>
</ui:UiBinder> 